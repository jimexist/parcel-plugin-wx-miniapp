{
  "name": "parcel-plugin-wx-miniapp",
  "version": "0.0.12",
  "description": "Parcel Plugin for Wechat MiniApp",
  "main": "index.js",
  "repository": "github:Jimexist/parcel-plugin-wx-miniapp",
  "author": "Jiayu Liu <etareduce@gmail.com>",
  "license": "MIT",
  "files": [
    "lib/"
  ],
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "clean": "rimraf lib/",
    "prepublishOnly": "npm run clean && npm run build",
    "prebuild": "npm run lint",
    "lint": "eslint src",
    "format": "prettier --write \"./{src,test}/**/*.{js,json,md}\"",
    "build": "babel src -d lib"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "parcel-bundler": "^1.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^23.3.0",
    "lint-staged": "^7.2.0",
    "parcel-bundler": "^1.7.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  },
  "dependencies": {}
}
